@prefix datacite: <http://purl.org/spar/datacite/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix fabio: <http://purl.org/spar/fabio/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix literal: <http://www.essepuntato.it/2010/06/literalreification/> .
@prefix pro: <http://purl.org/spar/pro/> .
@prefix prism: <http://prismstandard.org/namespaces/basic/2.0/> .
@prefix oco: <https://w3id.org/oc/ontology/> .
@prefix frbr: <http://purl.org/vocab/frbr/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# TEST DATA FOR SIMPLIFIED HERITRACE VERSION
# This data demonstrates the basic functionality without some properties
# that technicians will be asked to add during testing

# Sample Journal
<https://w3id.org/oc/meta/br/journal001> a fabio:Expression, fabio:Journal ;
    dcterms:title "Digital Humanities Quarterly" ;
    datacite:hasIdentifier [
        a datacite:Identifier ;
        datacite:usesIdentifierScheme datacite:issn ;
        literal:hasLiteralValue "1938-4122"
    ] .
    # NOTE: Missing dcterms:description - technicians should add this

# Sample Authors
<https://w3id.org/oc/meta/ra/author001> a foaf:Agent ;
    foaf:givenName "Maria" ;
    foaf:familyName "Rossi" .

<https://w3id.org/oc/meta/ra/author002> a foaf:Agent ;
    foaf:name "John Smith" .

<https://w3id.org/oc/meta/ra/author003> a foaf:Agent ;
    foaf:givenName "Anna" ;
    foaf:familyName "Bianchi" .

# Sample Author Roles
<https://w3id.org/oc/meta/ar/role001> a pro:RoleInTime ;
    pro:withRole pro:author ;
    pro:isHeldBy <https://w3id.org/oc/meta/ra/author001> ;
    oco:hasNext <https://w3id.org/oc/meta/ar/role002> .

<https://w3id.org/oc/meta/ar/role002> a pro:RoleInTime ;
    pro:withRole pro:author ;
    pro:isHeldBy <https://w3id.org/oc/meta/ra/author002> ;
    oco:hasNext <https://w3id.org/oc/meta/ar/role003> .

<https://w3id.org/oc/meta/ar/role003> a pro:RoleInTime ;
    pro:withRole pro:author ;
    pro:isHeldBy <https://w3id.org/oc/meta/ra/author003> .

# Sample Journal Article 1 (missing abstract, keywords, page info)
<https://w3id.org/oc/meta/br/article001> a fabio:Expression, fabio:JournalArticle ;
    dcterms:title "Machine Learning Applications in Cultural Heritage" ;
    datacite:hasIdentifier [
        a datacite:Identifier ;
        datacite:usesIdentifierScheme datacite:doi ;
        literal:hasLiteralValue "10.1162/dh_a_00456"
    ] ;
    prism:publicationDate "2023"^^xsd:gYear ;
    pro:isDocumentContextFor <https://w3id.org/oc/meta/ar/role001> ;
    frbr:partOf <https://w3id.org/oc/meta/br/journal001> .
    # MISSING PROPERTIES (technicians should add these):
    # - dcterms:abstract
    # - prism:keyword
    # - frbr:embodiment (page information)

# Sample Journal Article 2 (also missing key properties)
<https://w3id.org/oc/meta/br/article002> a fabio:Expression, fabio:JournalArticle ;
    dcterms:title "Digital Archives and Semantic Web Technologies" ;
    datacite:hasIdentifier [
        a datacite:Identifier ;
        datacite:usesIdentifierScheme datacite:doi ;
        literal:hasLiteralValue "10.1162/dh_a_00457"
    ] ;
    prism:publicationDate "2023"^^xsd:gYear ;
    pro:isDocumentContextFor <https://w3id.org/oc/meta/ar/role002> ;
    frbr:partOf <https://w3id.org/oc/meta/br/journal001> .
    # MISSING PROPERTIES (technicians should add these):
