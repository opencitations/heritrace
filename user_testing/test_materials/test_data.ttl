    # - frbr:embodiment (page information)

# Sample data for validation testing (invalid formats)
<https://w3id.org/oc/meta/br/article_invalid> a fabio:Expression, fabio:JournalArticle ;
    dcterms:title "Test Article with Invalid DOI" ;
    datacite:hasIdentifier [
        a datacite:Identifier ;
        datacite:usesIdentifierScheme datacite:doi ;
        literal:hasLiteralValue "invalid-doi-format"  # This should fail validation
    ] .

# COMPLETE EXAMPLES (what technicians should achieve)
# These show the expected result after adding missing properties

# Example of complete article with abstract, keywords, and page info
<https://w3id.org/oc/meta/br/article_complete> a fabio:Expression, fabio:JournalArticle ;
    dcterms:title "Complete Example: Digital Preservation Strategies" ;
    dcterms:abstract "This article explores various digital preservation strategies for cultural heritage institutions, focusing on metadata standards and long-term access solutions." ;
    prism:keyword "digital preservation", "cultural heritage", "metadata standards" ;
    datacite:hasIdentifier [
        a datacite:Identifier ;
        datacite:usesIdentifierScheme datacite:doi ;
        literal:hasLiteralValue "10.1162/dh_a_00458"
    ] ;
    prism:publicationDate "2023"^^xsd:gYear ;
    pro:isDocumentContextFor <https://w3id.org/oc/meta/ar/role003> ;
    frbr:partOf <https://w3id.org/oc/meta/br/journal001> ;
    frbr:embodiment [
        a fabio:Manifestation ;
        prism:startingPage "45" ;
        prism:endingPage "72"
    ] .

# Example of complete journal with description
<https://w3id.org/oc/meta/br/journal_complete> a fabio:Expression, fabio:Journal ;
    dcterms:title "Complete Journal Example" ;
    dcterms:description "A peer-reviewed journal focusing on digital humanities research and applications." ;
    datacite:hasIdentifier [
        a datacite:Identifier ;
        datacite:usesIdentifierScheme datacite:issn ;
        literal:hasLiteralValue "2641-3337"
    ] . 